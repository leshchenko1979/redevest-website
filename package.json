{
    "name": "redevest-website",
    "version": "1.0.0",
    "description": "Corporate website for Redevest - Real Estate Investment and Development",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "build": "node build.js",
        "watch": "tailwindcss build src/input.css -o dist/output.css --content \"src/**/*.{html,js}\" --watch",
        "dev": "npm run watch",
        "preview": "cd dist && python3 -m http.server 8080",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.19",
        "caniuse-lite": "^1.0.30001765",
        "fs-extra": "^11.2.0",
        "gray-matter": "^4.0.3",
        "jest": "^30.2.0",
        "postcss": "^8.4.38",
        "rehype-stringify": "^10.0.0",
        "remark": "^15.0.1",
        "remark-custom-blocks": "^2.5.0",
        "remark-gfm": "^4.0.0",
        "remark-html": "^16.0.1",
        "remark-rehype": "^11.1.0",
        "sharp": "^0.33.3",
        "tailwindcss": "^3.4.4",
        "unist-util-visit": "^5.0.0"
    },
    "dependencies": {
        "marked": "^17.0.1"
    }
}